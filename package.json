{
  "name": "@xq-stencil/node-mocha-chai-lib",
  "version": "1.2.3",
  "description": "A library stencil based on node and test used mocha and chai",
  "main": "lib/index.js",
  "engines": {
    "node": ">= 6"
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "clean": "rm -rf ./lib",
    "babel": "babel src --out-dir lib --copy-files",
    "babel:watch": "watch 'yarn babel' ./src",
    "build": "yarn clean && yarn babel",
    "lint": "eslint src test",
    "test": "nyc mocha test/",
    "test:watch": "watch 'yarn test' ./src ./test"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.5",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/preset-env": "^7.1.5",
    "@babel/register": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-plugin-istanbul": "^5.0.1",
    "chai": "^4.2.0",
    "eslint": "^5.9.0",
    "mocha": "^5.2.0",
    "mochawesome": "^3.1.1",
    "nyc": "^13.1.0",
    "watch": "^1.0.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xqstencils/node-mocha-chai-lib.git"
  },
  "keywords": [
    "lib",
    "node",
    "chai",
    "babel",
    "mocha",
    "eslint",
    "security",
    "circleci"
  ],
  "author": "Xiaoqi Cao<xqcao@xqcao.com>",
  "contributors": [
    "Xiaoqi Cao"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/xqstencils/node-mocha-chai-lib/issues"
  },
  "homepage": "https://github.com/xqstencils/node-mocha-chai-lib#readme"
}
